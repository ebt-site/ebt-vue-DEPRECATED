        "segMap": {
          "an1.394:0.2": {
            "scid": "an1.394:0.2",
            "pli": "29. Aparaaccharāsaṅghātavagga ",
            "en": "Another Chapter on a <span class=\"ebt-matched\">Finger-Snap</span> ",
            "matched": true
          },
          "an1.394:1.1": {
            "scid": "an1.394:1.1",
            "pli": "“Accharāsaṅghātamattampi ce, bhikkhave, bhikkhu paṭhamaṁ jhānaṁ bhāveti, ayaṁ vuccati, bhikkhave: ",
            "en": "“If, mendicants, a mendicant develops the first absorption, even as long as a <span class=\"ebt-matched\">finger-snap</span>, they are called ",
            "matched": true
          },
          "an1.401:1.1": {
            "scid": "an1.401:1.1",
            "pli": "upekkhaṁ cetovimuttiṁ bhāveti …pe…. ",
            "en": "or the heart’s release by equanimity, even as long as a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          },
          "an1.405:1.1": {
            "scid": "an1.405:1.1",
            "pli": "dhammesu dhammānupassī viharati ātāpī sampajāno satimā vineyya loke abhijjhādomanassaṁ. ",
            "en": "principles—keen, aware, and mindful, rid of covetousness and displeasure for the world, even for the time of a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          },
          "an1.406:1.1": {
            "scid": "an1.406:1.1",
            "pli": "Anuppannānaṁ pāpakānaṁ akusalānaṁ dhammānaṁ anuppādāya chandaṁ janeti vāyamati vīriyaṁ ārabhati cittaṁ paggaṇhāti padahati; ",
            "en": "If they generate enthusiasm, try, make an effort, exert the mind, and strive so that bad, unskillful qualities don’t arise, even for the time of a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          },
          "an1.407:1.1": {
            "scid": "an1.407:1.1",
            "pli": "uppannānaṁ pāpakānaṁ akusalānaṁ dhammānaṁ pahānāya chandaṁ janeti vāyamati vīriyaṁ ārabhati cittaṁ paggaṇhāti padahati. ",
            "en": "If they generate enthusiasm, try, make an effort, exert the mind, and strive so that bad, unskillful qualities that have arisen are given up, even for the time of a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          },
          "an1.408:1.1": {
            "scid": "an1.408:1.1",
            "pli": "Anuppannānaṁ kusalānaṁ dhammānaṁ uppādāya chandaṁ janeti vāyamati vīriyaṁ ārabhati cittaṁ paggaṇhāti padahati; ",
            "en": "If they generate enthusiasm, try, make an effort, exert the mind, and strive so that skillful qualities that have not arisen do arise, even for the time of a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          },
          "an1.409:1.1": {
            "scid": "an1.409:1.1",
            "pli": "uppannānaṁ kusalānaṁ dhammānaṁ ṭhitiyā asammosāya bhiyyobhāvāya vepullāya bhāvanāya pāripūriyā chandaṁ janeti vāyamati vīriyaṁ ārabhati cittaṁ paggaṇhāti padahati. ",
            "en": "If they generate enthusiasm, try, make an effort, exert the mind, and strive so that skillful qualities that have arisen remain, are not lost, but increase, mature, and are fulfilled by development, even for the time of a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          },
          "an1.413:1.1": {
            "scid": "an1.413:1.1",
            "pli": "vīmaṁsāsamādhipadhānasaṅkhārasamannāgataṁ iddhipādaṁ bhāveti …. ",
            "en": "the basis of psychic power that has immersion due to inquiry, and active effort, even for the time of a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          },
          "an1.418:1.1": {
            "scid": "an1.418:1.1",
            "pli": "paññindriyaṁ bhāveti …. ",
            "en": "the faculty of wisdom, even for the time of a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          },
          "an1.423:1.1": {
            "scid": "an1.423:1.1",
            "pli": "paññābalaṁ bhāveti …. ",
            "en": "the power of wisdom, even for the time of a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          },
          "an1.430:1.1": {
            "scid": "an1.430:1.1",
            "pli": "upekkhāsambojjhaṅgaṁ bhāveti …. ",
            "en": "the awakening factor of equanimity, even for the time of a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          },
          "an1.438:1.1": {
            "scid": "an1.438:1.1",
            "pli": "sammāsamādhiṁ bhāveti …. ",
            "en": "right immersion, even for the time of a <span class=\"ebt-matched\">finger-snap</span> … ",
            "matched": true
          }
