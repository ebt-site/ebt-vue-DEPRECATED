        "segMap": {
          "mn69:10.1": {
            "scid": "mn69:10.1",
            "pli": "Āraññikenāvuso, bhikkhunā indriyesu guttadvārena bhavitabbaṁ. ",
            "en": "A wilderness monk should <span class=\"ebt-matched\">guard the sense doors</span>. ",
            "matched": true
          },
          "mn69:10.3": {
            "scid": "mn69:10.3",
            "pli": "‘Kiṁ panimassāyasmato āraññikassa ekassāraññe serivihārena yo ayamāyasmā indriyesu aguttadvāro’ti—",
            "en": "‘What’s the point of this wilderness venerable’s staying alone and autonomous in the wilderness, since he doesn’t <span class=\"ebt-matched\">guard the sense doors</span>?’ ",
            "matched": true
          },
          "mn69:10.5": {
            "scid": "mn69:10.5",
            "pli": "Tasmā āraññikena bhikkhunā indriyesu guttadvārena bhavitabbaṁ. ",
            "en": "That’s why a wilderness monk should <span class=\"ebt-matched\">guard the sense doors</span>. ",
            "matched": true
          }
        },
