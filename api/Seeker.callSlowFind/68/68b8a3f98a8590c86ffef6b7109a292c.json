        "author": "Bhikkhu Sujato",
        "segMap": {
          "sn35.132:5.1": {
            "scid": "sn35.132:5.1",
            "pli": "Aguttadvārassa bhavanti moghā, ",
            "en": "All is vain for someone <j>who doesn’t <span class=\"ebt-matched\">guard the sense doors</span>, ",
            "matched": true
          },
          "sn35.132:12.1": {
            "scid": "sn35.132:12.1",
            "pli": "“‘Aguttadvāro’ti bhavaṁ kaccāno āha. ",
            "en": "“Mister Kaccāna spoke of someone who doesn’t <span class=\"ebt-matched\">guard the sense doors</span>. ",
            "matched": true
          },
          "sn35.132:12.2": {
            "scid": "sn35.132:12.2",
            "pli": "Kittāvatā nu kho, bho kaccāna, aguttadvāro hotī”ti? ",
            "en": "How do you define someone who doesn’t <span class=\"ebt-matched\">guard the sense doors</span>?” ",
            "matched": true
          },
          "sn35.132:12.11": {
            "scid": "sn35.132:12.11",
            "pli": "Evaṁ kho, brāhmaṇa, aguttadvāro hotī”ti. ",
            "en": "That’s how someone doesn’t <span class=\"ebt-matched\">guard the sense doors</span>.” ",
            "matched": true
          },
          "sn35.132:13.1": {
            "scid": "sn35.132:13.1",
            "pli": "‘Guttadvāro’ti bhavaṁ kaccāno āha. ",
            "en": "You also spoke of someone who does <span class=\"ebt-matched\">guard the sense doors</span>. ",
            "matched": true
          },
          "sn35.132:13.2": {
            "scid": "sn35.132:13.2",
            "pli": "Kittāvatā nu kho, bho kaccāna, guttadvāro hotī”ti? ",
            "en": "How do you define someone who does <span class=\"ebt-matched\">guard the sense doors</span>?” ",
            "matched": true
          }
        },
