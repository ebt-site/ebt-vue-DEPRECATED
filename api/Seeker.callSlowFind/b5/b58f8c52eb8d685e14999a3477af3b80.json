      {
        "author": "Bhikkhu Sujato",
        "segMap": {
          "sn46.28:0.3": {
            "scid": "sn46.28:0.3",
            "pli": "Nibbedhabhāgiyasutta ",
            "en": "Helping <span class=\"ebt-matched\">Penetration</span> ",
            "matched": true
          },
          "sn46.28:1.1": {
            "scid": "sn46.28:1.1",
            "pli": "“Nibbedhabhāgiyaṁ vo, bhikkhave, maggaṁ desessāmi; ",
            "en": "“Mendicants, I will teach you a path that helps <span class=\"ebt-matched\">penetration</span>. ",
            "matched": true
          },
          "sn46.28:1.3": {
            "scid": "sn46.28:1.3",
            "pli": "Katamo ca, bhikkhave, nibbedhabhāgiyo maggo? ",
            "en": "And what is the path that helps <span class=\"ebt-matched\">penetration</span>? ",
            "matched": true
          },
          "sn46.28:1.9": {
            "scid": "sn46.28:1.9",
            "pli": "“kathaṁ bhāvitā nu kho, bhante, satta bojjhaṅgā kathaṁ bahulīkatā nibbedhāya saṁvattantī”ti? ",
            "en": "“Sir, how are the seven awakening factors developed and cultivated so as to lead to <span class=\"ebt-matched\">penetration</span>?” ",
            "matched": true
          },
          "sn46.28:2.9": {
            "scid": "sn46.28:2.9",
            "pli": "Evaṁ bhāvitā kho, udāyi, satta bojjhaṅgā evaṁ bahulīkatā nibbedhāya saṁvattantī”ti. ",
            "en": "This is how are the seven awakening factors are developed and cultivated so as to lead to <span class=\"ebt-matched\">penetration</span>.” ",
            "matched": true
          }
        },
