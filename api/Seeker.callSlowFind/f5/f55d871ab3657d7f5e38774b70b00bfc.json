      {
        "author": "Bhikkhu Sujato",
        "segMap": {
          "an4.110:0.3": {
            "scid": "an4.110:0.3",
            "pli": "Āsīvisasutta ",
            "en": "<span class=\"ebt-matched\">Viper</span>s ",
            "matched": true
          },
          "an4.110:1.1": {
            "scid": "an4.110:1.1",
            "pli": "“Cattārome, bhikkhave, āsīvisā. ",
            "en": "“Mendicants, there are these four kinds of <span class=\"ebt-matched\">viper</span>. ",
            "matched": true
          },
          "an4.110:1.7": {
            "scid": "an4.110:1.7",
            "pli": "Ime kho, bhikkhave, cattāro āsīvisā. ",
            "en": "These are the four kinds of <span class=\"ebt-matched\">viper</span>. ",
            "matched": true
          },
          "an4.110:1.8": {
            "scid": "an4.110:1.8",
            "pli": "Evamevaṁ kho, bhikkhave, cattāro āsīvisūpamā puggalā santo saṁvijjamānā lokasmiṁ. ",
            "en": "In the same way, these four individuals similar to <span class=\"ebt-matched\">viper</span>s are found in the world. ",
            "matched": true
          },
          "an4.110:2.5": {
            "scid": "an4.110:2.5",
            "pli": "Seyyathāpi so, bhikkhave, āsīviso āgataviso, na ghoraviso; ",
            "en": "That individual is like a <span class=\"ebt-matched\">viper</span> whose venom is fast-acting but not lethal. ",
            "matched": true
          },
          "an4.110:5.7": {
            "scid": "an4.110:5.7",
            "pli": "Ime kho, bhikkhave, cattāro āsīvisūpamā puggalā santo saṁvijjamānā lokasmin”ti. ",
            "en": "These four individuals similar to <span class=\"ebt-matched\">viper</span>s are found in the world.” ",
            "matched": true
          }
        },
