        "author": "Bhikkhu Sujato",
        "segMap": {
          "an3.63:3.9": {
            "scid": "an3.63:3.9",
            "pli": "Yāni tāni, bho gotama, uccāsayanamahāsayanāni, seyyathidaṁ—",
            "en": "Surely the worthy Gotama gets when he wants, without trouble or difficulty, various kinds of high and <span class=\"ebt-matched\">luxurious bed</span>ding, such as: ",
            "matched": true
          },
          "an3.63:4.1": {
            "scid": "an3.63:4.1",
            "pli": "“Yāni kho pana tāni, brāhmaṇa, uccāsayanamahāsayanāni, seyyathidaṁ—",
            "en": "“Brahmin, these various kinds of high and <span class=\"ebt-matched\">luxurious bed</span>ding ",
            "matched": true
          },
          "an3.63:5.1": {
            "scid": "an3.63:5.1",
            "pli": "Tīṇi kho, imāni, brāhmaṇa, uccāsayanamahāsayanāni, yesāhaṁ etarahi nikāmalābhī akicchalābhī akasiralābhī. ",
            "en": "There are, brahmin, these three high and <span class=\"ebt-matched\">luxurious bed</span>s that I get these days when I want, without trouble or difficulty. ",
            "matched": true
          },
          "an3.63:5.3": {
            "scid": "an3.63:5.3",
            "pli": "Dibbaṁ uccāsayanamahāsayanaṁ, brahmaṁ uccāsayanamahāsayanaṁ, ariyaṁ uccāsayanamahāsayanaṁ. ",
            "en": "The heavenly high and <span class=\"ebt-matched\">luxurious bed</span>, the divine high and <span class=\"ebt-matched\">luxurious bed</span>, and the noble high and <span class=\"ebt-matched\">luxurious bed</span>. ",
            "matched": true
          },
          "an3.63:5.4": {
            "scid": "an3.63:5.4",
            "pli": "Imāni kho, brāhmaṇa, tīṇi uccāsayanamahāsayanāni, yesāhaṁ etarahi nikāmalābhī akicchalābhī akasiralābhī”ti. ",
            "en": "These are the three high and <span class=\"ebt-matched\">luxurious bed</span>s that I get these days when I want, without trouble or difficulty.” ",
            "matched": true
          },
          "an3.63:6.1": {
            "scid": "an3.63:6.1",
            "pli": "“Katamaṁ pana taṁ, bho gotama, dibbaṁ uccāsayanamahāsayanaṁ, yassa bhavaṁ gotamo etarahi nikāmalābhī akicchalābhī akasiralābhī”ti? ",
            "en": "“But what, worthy Gotama, is the heavenly high and <span class=\"ebt-matched\">luxurious bed</span>?” ",
            "matched": true
          },
          "an3.63:6.13": {
            "scid": "an3.63:6.13",
            "pli": "Idaṁ kho, brāhmaṇa, dibbaṁ uccāsayanamahāsayanaṁ, yassāhaṁ etarahi nikāmalābhī akicchalābhī akasiralābhī”ti. ",
            "en": "This is the heavenly high and <span class=\"ebt-matched\">luxurious bed</span> that I get these days when I want, without trouble or difficulty.” ",
            "matched": true
          },
          "an3.63:7.2": {
            "scid": "an3.63:7.2",
            "pli": "Ko cañño evarūpassa dibbassa uccāsayanamahāsayanassa nikāmalābhī bhavissati akicchalābhī akasiralābhī, aññatra bhotā gotamena. ",
            "en": "Who but the worthy Gotama could get such a heavenly high and <span class=\"ebt-matched\">luxurious bed</span> when he wants, without trouble or difficulty? ",
            "matched": true
          },
