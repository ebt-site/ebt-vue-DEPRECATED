        "segMap": {
          "sn7.12:1.6": {
            "scid": "sn7.12:1.6",
            "pli": "“pakaṭṭhakoyaṁ samaṇo gotamo punappunaṁ āgacchatī”ti. ",
            "en": "“This insatiable ascetic Gotama keeps coming back <span class=\"ebt-matched\">again and again</span>!” ",
            "matched": true
          },
          "sn7.12:2.1": {
            "scid": "sn7.12:2.1",
            "pli": "“Punappunañceva vapanti bījaṁ, ",
            "en": "“<span class=\"ebt-matched\">Again and again</span>, they sow the seed; ",
            "matched": true
          },
          "sn7.12:2.2": {
            "scid": "sn7.12:2.2",
            "pli": "Punappunaṁ vassati devarājā; ",
            "en": "<span class=\"ebt-matched\">again and again</span>, the king of the heavens sends rain; ",
            "matched": true
          },
          "sn7.12:2.3": {
            "scid": "sn7.12:2.3",
            "pli": "Punappunaṁ khettaṁ kasanti kassakā, ",
            "en": "<span class=\"ebt-matched\">again and again</span>, farmers plough the field; ",
            "matched": true
          },
          "sn7.12:2.4": {
            "scid": "sn7.12:2.4",
            "pli": "Punappunaṁ dhaññamupeti raṭṭhaṁ. ",
            "en": "<span class=\"ebt-matched\">again and again</span>, grain is produced for the nation. ",
            "matched": true
          },
          "sn7.12:3.1": {
            "scid": "sn7.12:3.1",
            "pli": "Punappunaṁ yācakā yācayanti, ",
            "en": "<span class=\"ebt-matched\">Again and again</span>, the beggars beg; ",
            "matched": true
          },
          "sn7.12:3.2": {
            "scid": "sn7.12:3.2",
            "pli": "Punappunaṁ dānapatī dadanti; ",
            "en": "<span class=\"ebt-matched\">again and again</span>, the donors give. ",
            "matched": true
          },
          "sn7.12:3.3": {
            "scid": "sn7.12:3.3",
            "pli": "Punappunaṁ dānapatī daditvā, ",
            "en": "<span class=\"ebt-matched\">Again and again</span>, when the donors have given, ",
            "matched": true
          },
          "sn7.12:3.4": {
            "scid": "sn7.12:3.4",
            "pli": "Punappunaṁ saggamupenti ṭhānaṁ. ",
            "en": "<span class=\"ebt-matched\">again and again</span>, they take their place in heaven. ",
            "matched": true
          },
          "sn7.12:4.1": {
            "scid": "sn7.12:4.1",
            "pli": "Punappunaṁ khīranikā duhanti, ",
            "en": "<span class=\"ebt-matched\">Again and again</span>, dairy farmers milk; ",
            "matched": true
          },
          "sn7.12:4.2": {
            "scid": "sn7.12:4.2",
            "pli": "Punappunaṁ vaccho upeti mātaraṁ; ",
            "en": "<span class=\"ebt-matched\">again and again</span>, a calf cleaves to its mother; ",
            "matched": true
          },
          "sn7.12:4.3": {
            "scid": "sn7.12:4.3",
            "pli": "Punappunaṁ kilamati phandati ca, ",
            "en": "<span class=\"ebt-matched\">again and again</span>, oppressing and intimidating; ",
            "matched": true
          },
          "sn7.12:4.4": {
            "scid": "sn7.12:4.4",
            "pli": "Punappunaṁ gabbhamupeti mando. ",
            "en": "that dullard returns to the womb <span class=\"ebt-matched\">again and again</span>. ",
            "matched": true
          },
          "sn7.12:5.1": {
            "scid": "sn7.12:5.1",
            "pli": "Punappunaṁ jāyati mīyati ca, ",
            "en": "<span class=\"ebt-matched\">Again and again</span>, you’re reborn and die; ",
            "matched": true
          },
          "sn7.12:5.2": {
            "scid": "sn7.12:5.2",
            "pli": "Punappunaṁ sivathikaṁ haranti; ",
            "en": "<span class=\"ebt-matched\">again and again</span>, you get carried to a charnel ground. ",
            "matched": true
          },
          "sn7.12:5.4": {
            "scid": "sn7.12:5.4",
            "pli": "Na punappunaṁ jāyati bhūripañño”ti. ",
            "en": "one of vast wisdom is not reborn <span class=\"ebt-matched\">again and again</span>.” ",
            "matched": true
          }
        },
