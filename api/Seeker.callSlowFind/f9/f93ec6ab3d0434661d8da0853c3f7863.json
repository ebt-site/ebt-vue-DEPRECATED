        "segMap": {
          "mn146:6.27": {
            "scid": "mn146:6.27",
            "pli": "‘itipime cha ajjhattikā āyatanā aniccā’”ti. ",
            "en": "‘So these six <span class=\"ebt-matched\">interior sense fields</span> are impermanent.’” ",
            "matched": true
          },
          "mn146:9.11": {
            "scid": "mn146:9.11",
            "pli": "‘cha khome ajjhattikā āyatanā aniccā; ",
            "en": "‘These six <span class=\"ebt-matched\">interior sense fields</span> are impermanent. ",
            "matched": true
          },
          "mn146:9.12": {
            "scid": "mn146:9.12",
            "pli": "yañca kho cha ajjhattike āyatane paṭicca paṭisaṁvedeti sukhaṁ vā dukkhaṁ vā adukkhamasukhaṁ vā taṁ niccaṁ dhuvaṁ sassataṁ avipariṇāmadhamman’ti; ",
            "en": "But the feeling—whether pleasant, painful, or neutral—that I experience due to these six <span class=\"ebt-matched\">interior sense fields</span> is permanent, lasting, eternal, and imperishable.’ ",
            "matched": true
          },
          "mn146:12.3": {
            "scid": "mn146:12.3",
            "pli": "‘antarā maṁsakāyo’ti kho, bhaginiyo, channetaṁ ajjhattikānaṁ āyatanānaṁ adhivacanaṁ; ",
            "en": "‘The inner flesh’ is a term for the six <span class=\"ebt-matched\">interior sense fields</span>. ",
            "matched": true
          }
        },
