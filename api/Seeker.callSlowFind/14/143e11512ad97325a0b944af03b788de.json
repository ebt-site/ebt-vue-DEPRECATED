        "author": "Sabbamitta",
        "segMap": {
          "an3.183-352:1.1": {
            "scid": "an3.183-352:1.1",
            "pli": "“Rāgassa, bhikkhave, abhiññāya tayo dhammā bhāvetabbā. ",
            "de": "„Um Gier unmittelbar zu erkennen, müssen <span class=\"ebt-matched\">drei Dinge entwickelt werden</span>. ",
            "en": "“For insight into greed, three things should be developed. ",
            "matched": true
          },
          "an3.183-352:1.4": {
            "scid": "an3.183-352:1.4",
            "pli": "rāgassa, bhikkhave, abhiññāya ime tayo dhammā bhāvetabbā. (…) ",
            "de": "Um Gier unmittelbar zu erkennen, müssen diese <span class=\"ebt-matched\">drei Dinge entwickelt werden</span>. ",
            "en": "For insight into greed, these three things should be developed. ",
            "matched": true
          },
          "an3.183-352:3.26": {
            "scid": "an3.183-352:3.26",
            "pli": "paṭinissaggāya ime tayo dhammā bhāvetabbā”ti. ",
            "de": "Um Nachlässigkeit loszulassen müssen diese <span class=\"ebt-matched\">drei Dinge entwickelt werden</span>.“ ",
            "en": "For the letting go of negligence, these three things should be developed.” ",
            "matched": true
          }
        },
        "author_uid": "sabbamitta",
