        "author": "Bhikkhu Sujato",
        "segMap": {
          "sn23.3:0.3": {
            "scid": "sn23.3:0.3",
            "pli": "Bhavanettisutta ",
            "en": "The <span class=\"ebt-matched\">Conduit To Rebirth</span> ",
            "matched": true
          },
          "sn23.3:1.3": {
            "scid": "sn23.3:1.3",
            "pli": "“‘bhavanettinirodho, bhavanettinirodho’ti, bhante, vuccati. ",
            "en": "“Sir, they speak of this thing called ‘the cessation of the <span class=\"ebt-matched\">conduit to rebirth</span>’. ",
            "matched": true
          },
          "sn23.3:1.4": {
            "scid": "sn23.3:1.4",
            "pli": "Katamā nu kho, bhante, bhavanetti, katamo bhavanettinirodho”ti? ",
            "en": "What is the <span class=\"ebt-matched\">conduit to rebirth</span>? And what is the cessation of the <span class=\"ebt-matched\">conduit to rebirth</span>?” ",
            "matched": true
          },
          "sn23.3:1.6": {
            "scid": "sn23.3:1.6",
            "pli": "ayaṁ vuccati bhavanetti. ",
            "en": "this is called the <span class=\"ebt-matched\">conduit to rebirth</span>. ",
            "matched": true
          },
          "sn23.3:1.7": {
            "scid": "sn23.3:1.7",
            "pli": "Tesaṁ nirodho bhavanettinirodho. ",
            "en": "Their cessation is the cessation of the <span class=\"ebt-matched\">conduit to rebirth</span>. ",
            "matched": true
          },
          "sn23.3:1.12": {
            "scid": "sn23.3:1.12",
            "pli": "ayaṁ vuccati bhavanetti. ",
            "en": "this is called the <span class=\"ebt-matched\">conduit to rebirth</span>. ",
            "matched": true
          },
          "sn23.3:1.13": {
            "scid": "sn23.3:1.13",
            "pli": "Tesaṁ nirodho bhavanettinirodho”ti. ",
            "en": "Their cessation is the cessation of the <span class=\"ebt-matched\">conduit to rebirth</span>.” ",
            "matched": true
          }
        },
