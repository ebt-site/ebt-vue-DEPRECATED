      {
        "author": "Bhikkhu Sujato",
        "segMap": {
          "ud8.8:5.1": {
            "scid": "ud8.8:5.1",
            "pli": "“Yesaṁ kho, visākhe, sataṁ piyāni, sataṁ tesaṁ dukkhāni; ",
            "en": "“Those who have a hundred <span class=\"ebt-matched\">loved one</span>s, Visākhā, have a hundred sufferings. ",
            "matched": true
          },
          "ud8.8:5.2": {
            "scid": "ud8.8:5.2",
            "pli": "yesaṁ navuti piyāni, navuti tesaṁ dukkhāni; ",
            "en": "Those who have ninety <span class=\"ebt-matched\">loved one</span>s, ",
            "matched": true
          },
          "ud8.8:5.19": {
            "scid": "ud8.8:5.19",
            "pli": "yesaṁ ekaṁ piyaṁ, ekaṁ tesaṁ dukkhaṁ; ",
            "en": "or one <span class=\"ebt-matched\">loved one</span> have one suffering. ",
            "matched": true
          },
          "ud8.8:5.20": {
            "scid": "ud8.8:5.20",
            "pli": "yesaṁ natthi piyaṁ, natthi tesaṁ dukkhaṁ, asokā te virajā anupāyāsāti vadāmī”ti. ",
            "en": "Those who have no <span class=\"ebt-matched\">loved one</span>s have no suffering. They are free of sorrow, stains, and anguish I say.” ",
            "matched": true
          },
          "ud8.8:7.4": {
            "scid": "ud8.8:7.4",
            "pli": "Piye asante na bhavanti ete. ",
            "en": "without <span class=\"ebt-matched\">loved one</span>s they do not occur. ",
            "matched": true
          },
          "ud8.8:8.1": {
            "scid": "ud8.8:8.1",
            "pli": "Tasmā hi te sukhino vītasokā, ",
            "en": "That’s why those who have no <span class=\"ebt-matched\">loved one</span>s <j>at all in the world ",
            "matched": true
          },
          "ud8.8:8.4": {
            "scid": "ud8.8:8.4",
            "pli": "Piyaṁ na kayirātha kuhiñci loke”ti. ",
            "en": "have no <span class=\"ebt-matched\">loved one</span>s in the world at all.” ",
            "matched": true
          }
        },
