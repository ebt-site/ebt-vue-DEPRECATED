        "segMap": {
          "dn21:2.2.23": {
            "scid": "dn21:2.2.23",
            "pli": "“Vitakko kho, devānaminda, papañcasaññāsaṅkhānidāno papañcasaññāsaṅkhāsamudayo papañcasaññāsaṅkhājātiko papañcasaññāsaṅkhāpabhavo; ",
            "en": "“<span class=\"ebt-matched\">Judgment</span>s driven by the proliferation of perceptions are the source of thoughts.” ",
            "matched": true
          },
          "dn21:2.3.1": {
            "scid": "dn21:2.3.1",
            "pli": "“Kathaṁ paṭipanno pana, mārisa, bhikkhu papañcasaññāsaṅkhānirodhasāruppagāminiṁ paṭipadaṁ paṭipanno hotī”ti? ",
            "en": "“But how does a mendicant fittingly practice for the cessation of <span class=\"ebt-matched\">judgment</span>s driven by the proliferation of perceptions?” ",
            "matched": true
          },
          "dn21:2.3.33": {
            "scid": "dn21:2.3.33",
            "pli": "Evaṁ paṭipanno kho, devānaminda, bhikkhu papañcasaññāsaṅkhānirodhasāruppagāminiṁ paṭipadaṁ paṭipanno hotī”ti. ",
            "en": "That’s how a mendicant fittingly practices for the cessation of <span class=\"ebt-matched\">judgment</span>s driven by the proliferation of perceptions.” ",
            "matched": true
          }
        },
