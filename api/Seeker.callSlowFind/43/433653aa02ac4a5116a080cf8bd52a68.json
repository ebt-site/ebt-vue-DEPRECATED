        "segMap": {
          "mn89:0.2": {
            "scid": "mn89:0.2",
            "pli": "Dhammacetiyasutta ",
            "en": "<span class=\"ebt-matched\">Shrines</span> to the Teaching ",
            "matched": true
          },
          "mn89:21.2": {
            "scid": "mn89:21.2",
            "pli": "“eso, bhikkhave, rājā pasenadi kosalo dhammacetiyāni bhāsitvā uṭṭhāyāsanā pakkanto. ",
            "en": "“Mendicants, before he got up and left, King Pasenadi spoke <span class=\"ebt-matched\">shrines</span> to the teaching. ",
            "matched": true
          },
          "mn89:21.3": {
            "scid": "mn89:21.3",
            "pli": "Uggaṇhatha, bhikkhave, dhammacetiyāni; ",
            "en": "Learn these <span class=\"ebt-matched\">shrines</span> to the teaching! ",
            "matched": true
          },
          "mn89:21.4": {
            "scid": "mn89:21.4",
            "pli": "pariyāpuṇātha, bhikkhave, dhammacetiyāni; ",
            "en": "Memorize these <span class=\"ebt-matched\">shrines</span> to the teaching! ",
            "matched": true
          },
          "mn89:21.5": {
            "scid": "mn89:21.5",
            "pli": "dhāretha, bhikkhave, dhammacetiyāni. ",
            "en": "Remember these <span class=\"ebt-matched\">shrines</span> to the teaching! ",
            "matched": true
          },
          "mn89:21.6": {
            "scid": "mn89:21.6",
            "pli": "Atthasaṁhitāni, bhikkhave, dhammacetiyāni ādibrahmacariyakānī”ti. ",
            "en": "These <span class=\"ebt-matched\">shrines</span> to the teaching are beneficial and relate to the fundamentals of the spiritual life.” ",
            "matched": true
          }
        },
