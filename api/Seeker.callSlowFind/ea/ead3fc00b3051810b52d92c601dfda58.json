      {
        "author": "Sabbamitta",
        "segMap": {
          "an4.110:0.3": {
            "scid": "an4.110:0.3",
            "pli": "Āsīvisasutta ",
            "de": "<span class=\"ebt-matched\">Giftschlange</span>n ",
            "en": "Vipers ",
            "matched": true
          },
          "an4.110:1.1": {
            "scid": "an4.110:1.1",
            "pli": "“Cattārome, bhikkhave, āsīvisā. ",
            "de": "„Mönche und Nonnen, es gibt vier <span class=\"ebt-matched\">Giftschlange</span>n. ",
            "en": "“Mendicants, there are these four kinds of viper. ",
            "matched": true
          },
          "an4.110:1.7": {
            "scid": "an4.110:1.7",
            "pli": "Ime kho, bhikkhave, cattāro āsīvisā. ",
            "de": "Das sind die vier <span class=\"ebt-matched\">Giftschlange</span>n. ",
            "en": "These are the four kinds of viper. ",
            "matched": true
          },
          "an4.110:1.8": {
            "scid": "an4.110:1.8",
            "pli": "Evamevaṁ kho, bhikkhave, cattāro āsīvisūpamā puggalā santo saṁvijjamānā lokasmiṁ. ",
            "de": "Ebenso gibt es in der Welt vier Menschen, die <span class=\"ebt-matched\">Giftschlange</span>n gleichen. ",
            "en": "In the same way, these four people similar to vipers are found in the world. ",
            "matched": true
          },
          "an4.110:2.5": {
            "scid": "an4.110:2.5",
            "pli": "Seyyathāpi so, bhikkhave, āsīviso āgataviso, na ghoraviso; ",
            "de": "Dieser Mensch gleicht einer <span class=\"ebt-matched\">Giftschlange</span>, deren Gift schnell wirkt, doch nicht tödlich ist. ",
            "en": "That person is like a viper whose venom is fast-acting but not lethal. ",
            "matched": true
          },
          "an4.110:5.5": {
            "scid": "an4.110:5.5",
            "pli": "Seyyathāpi so, bhikkhave, āsīviso nevāgataviso na ghoraviso; ",
            "de": "Dieser Mensch gleicht einer <span class=\"ebt-matched\">Giftschlange</span>, deren Gift weder schnell wirkt noch tödlich ist. ",
            "en": "",
            "matched": true
          },
          "an4.110:5.7": {
            "scid": "an4.110:5.7",
            "pli": "Ime kho, bhikkhave, cattāro āsīvisūpamā puggalā santo saṁvijjamānā lokasmin”ti. ",
            "de": "Das sind die vier Menschen, die <span class=\"ebt-matched\">Giftschlange</span>n gleichen, die man in der Welt findet.“ ",
            "en": "These four people similar to vipers are found in the world.” ",
            "matched": true
          }
        },
