        "author": "Sabbamitta",
        "segMap": {
          "an9.30:0.3": {
            "scid": "an9.30:0.3",
            "pli": "Āghātapaṭivinayasutta ",
            "de": "Sich <span class=\"ebt-matched\">von Groll los</span>machen ",
            "en": "Getting Rid of Resentment ",
            "matched": true
          },
          "an9.30:1.1": {
            "scid": "an9.30:1.1",
            "pli": "“Navayime, bhikkhave, āghātapaṭivinayā. ",
            "de": "„Mönche und Nonnen, es gibt neun Methoden, um sich <span class=\"ebt-matched\">von Groll los</span>zumachen. ",
            "en": "“Mendicants, there are these nine methods to get rid of resentment. ",
            "matched": true
          },
          "an9.30:1.3": {
            "scid": "an9.30:1.3",
            "pli": "‘Anatthaṁ me acari, taṁ kutettha labbhā’ti āghātaṁ paṭivineti; ",
            "de": "Beim Gedanken: ‚Dieser Mensch hat mir Unrecht getan, doch was kann ich da machen?‘ macht man sich <span class=\"ebt-matched\">von Groll los</span>. ",
            "en": "Thinking: ‘They harmed me, but what can I possibly do?’ you get rid of resentment. ",
            "matched": true
          },
          "an9.30:1.11": {
            "scid": "an9.30:1.11",
            "pli": "‘atthaṁ carissati, taṁ kutettha labbhā’ti āghātaṁ paṭivineti. ",
            "de": "Beim Gedanken: ‚Dieser Mensch wird jemandem helfen, der mir unlieb und verhasst ist, doch was kann ich da machen?‘ macht man sich <span class=\"ebt-matched\">von Groll los</span>. ",
            "en": "Thinking: ‘They will help someone I dislike, but what can I possibly do?’ you get rid of resentment. ",
            "matched": true
          },
          "an9.30:1.12": {
            "scid": "an9.30:1.12",
            "pli": "Ime kho, bhikkhave, nava āghātapaṭivinayā”ti. ",
            "de": "Das sind die neun Methoden, um sich <span class=\"ebt-matched\">von Groll los</span>zumachen.“ ",
            "en": "These are the nine methods to get rid of resentment.” ",
            "matched": true
          }
        },
