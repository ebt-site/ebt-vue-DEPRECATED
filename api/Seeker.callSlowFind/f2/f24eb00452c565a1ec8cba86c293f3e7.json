      {
        "author": "Bhikkhu Sujato",
        "segMap": {
          "an3.92:1.1": {
            "scid": "an3.92:1.1",
            "pli": "“Tīṇimāni, bhikkhave, kassakassa gahapatissa accāyikāni karaṇīyāni. ",
            "en": "“Mendicants, a <span class=\"ebt-matched\">farmer</span> has three urgent duties. ",
            "matched": true
          },
          "an3.92:1.3": {
            "scid": "an3.92:1.3",
            "pli": "Idha, bhikkhave, kassako gahapati sīghaṁ sīghaṁ khettaṁ sukaṭṭhaṁ karoti sumatikataṁ. ",
            "en": "A <span class=\"ebt-matched\">farmer</span> swiftly makes sure the field is well ploughed and tilled. ",
            "matched": true
          },
          "an3.92:1.6": {
            "scid": "an3.92:1.6",
            "pli": "Imāni kho, bhikkhave, tīṇi kassakassa gahapatissa accāyikāni karaṇīyāni. ",
            "en": "These are the three urgent duties of a <span class=\"ebt-matched\">farmer</span>. ",
            "matched": true
          },
          "an3.92:1.7": {
            "scid": "an3.92:1.7",
            "pli": "Tassa kho taṁ, bhikkhave, kassakassa gahapatissa natthi sā iddhi vā ānubhāvo vā: ",
            "en": "That <span class=\"ebt-matched\">farmer</span> has no special power or ability to say: ",
            "matched": true
          },
          "an3.92:1.9": {
            "scid": "an3.92:1.9",
            "pli": "Atha kho, bhikkhave, hoti so samayo yaṁ tassa kassakassa gahapatissa tāni dhaññāni utupariṇāmīni jāyantipi gabbhīnipi honti paccantipi. ",
            "en": "But there comes a time when that <span class=\"ebt-matched\">farmer</span>’s crops germinate, flower, and ripen as the seasons change. ",
            "matched": true
          }
        },
