        "author": "Bhikkhu Sujato",
        "segMap": {
          "an4.95:1.3": {
            "scid": "an4.95:1.3",
            "pli": "Nevattahitāya paṭipanno no parahitāya, ",
            "en": "One who <span class=\"ebt-matched\">practices to benefit</span> neither themselves nor others; ",
            "matched": true
          },
          "an4.95:1.4": {
            "scid": "an4.95:1.4",
            "pli": "parahitāya paṭipanno no attahitāya, ",
            "en": "one who <span class=\"ebt-matched\">practices to benefit</span> others, but not themselves; ",
            "matched": true
          },
          "an4.95:1.5": {
            "scid": "an4.95:1.5",
            "pli": "attahitāya paṭipanno no parahitāya, ",
            "en": "one who <span class=\"ebt-matched\">practices to benefit</span> themselves, but not others; and ",
            "matched": true
          },
          "an4.95:1.6": {
            "scid": "an4.95:1.6",
            "pli": "attahitāya ceva paṭipanno parahitāya ca. ",
            "en": "one who <span class=\"ebt-matched\">practices to benefit</span> both themselves and others. ",
            "matched": true
          },
          "an4.95:2.2": {
            "scid": "an4.95:2.2",
            "pli": "tathūpamāhaṁ, bhikkhave, imaṁ puggalaṁ vadāmi yvāyaṁ puggalo nevattahitāya paṭipanno no parahitāya. ",
            "en": "The person who <span class=\"ebt-matched\">practices to benefit</span> neither themselves nor others is like this, I say. ",
            "matched": true
          },
          "an4.95:3.1": {
            "scid": "an4.95:3.1",
            "pli": "Tatra, bhikkhave, yvāyaṁ puggalo parahitāya paṭipanno no attahitāya, ayaṁ imesaṁ dvinnaṁ puggalānaṁ abhikkantataro ca paṇītataro ca. ",
            "en": "The person who <span class=\"ebt-matched\">practices to benefit</span> others, but not themselves, is better than that. ",
            "matched": true
          },
          "an4.95:3.2": {
            "scid": "an4.95:3.2",
            "pli": "Tatra, bhikkhave, yvāyaṁ puggalo attahitāya paṭipanno no parahitāya, ayaṁ imesaṁ tiṇṇaṁ puggalānaṁ abhikkantataro ca paṇītataro ca. ",
            "en": "The person who <span class=\"ebt-matched\">practices to benefit</span> themselves, but not others, is better than both of those. ",
            "matched": true
          },
          "an4.95:3.3": {
            "scid": "an4.95:3.3",
            "pli": "Tatra, bhikkhave, yvāyaṁ puggalo attahitāya ceva paṭipanno parahitāya ca, ayaṁ imesaṁ catunnaṁ puggalānaṁ aggo ca seṭṭho ca pāmokkho ca uttamo ca pavaro ca. ",
            "en": "But the person who <span class=\"ebt-matched\">practices to benefit</span> both themselves and others is the foremost, best, chief, highest, and finest of the four. ",
            "matched": true
          },
          "an4.95:4.2": {
            "scid": "an4.95:4.2",
            "pli": "evamevaṁ kho, bhikkhave, yvāyaṁ puggalo attahitāya ceva paṭipanno parahitāya ca, ayaṁ imesaṁ catunnaṁ puggalānaṁ aggo ca seṭṭho ca pāmokkho ca uttamo ca pavaro ca. ",
            "en": "In the same way, the person who <span class=\"ebt-matched\">practices to benefit</span> both themselves and others is the foremost, best, chief, highest, and finest of the four. ",
            "matched": true
          }
        },
